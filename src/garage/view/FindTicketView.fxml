<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="garage.view.FindTicketViewController">
   <children>
      <HBox alignment="CENTER_RIGHT" spacing="15.0">
         <VBox.margin>
            <Insets right="50.0" top="20.0" />
         </VBox.margin>
         <children>
            <Label text="Filter:" />
            <TextField fx:id="filterField" alignment="CENTER_RIGHT" prefWidth="200.0" />
         </children>
      </HBox>
      <TableView fx:id="table" prefHeight="400.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="ticketNumCol" prefWidth="150.0" text="Ticket #" />
          <TableColumn fx:id="levelCol" minWidth="0.0" prefWidth="65.0" text="Level" />
            <TableColumn fx:id="spaceCol" prefWidth="65.0" text="Space" />
            <TableColumn fx:id="nameCol" prefWidth="150.0" text="Name" />
            <TableColumn fx:id="plateNumCol" prefWidth="120.0" text="Plate #" />
            <TableColumn fx:id="phoneNumCol" prefWidth="150.0" text="Phone #" />
        </columns>
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
      </TableView>
   </children>
</VBox>
